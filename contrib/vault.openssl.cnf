# this file is NOT intended for manual use
# use the oxi-gen-vault tool found at usr/share/openxpki/bin

[ req ]
default_bits = 3072
distinguished_name = req_distinguished_name
encrypt_key = no
req_extensions = v3_datavault_extensions
x509_extensions = v3_datavault_extensions
prompt = no

[ req_distinguished_name ]
commonName=DataVault

[ v3_datavault_extensions ]
subjectKeyIdentifier    = hash
keyUsage                = keyEncipherment
basicConstraints        = CA:FALSE
authorityKeyIdentifier  = keyid:always,issuer

